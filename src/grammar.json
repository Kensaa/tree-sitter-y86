{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "y86",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "line"
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z][0-9a-zA-Z_]*"
    },
    "_newline": {
      "type": "PATTERN",
      "value": "[\\n]"
    },
    "line": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "label"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "instruction"
                },
                {
                  "type": "SYMBOL",
                  "name": "directive"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "comment"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        }
      ]
    },
    "instruction": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "arg_list"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "directive": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "arg_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "arg"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "SYMBOL",
                  "name": "arg"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "arg": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "index"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "register"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "(0x[0-9a-fA-F]+)|(-?[0-9]+)"
    },
    "register": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "%"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "eax"
            },
            {
              "type": "STRING",
              "value": "ecx"
            },
            {
              "type": "STRING",
              "value": "edx"
            },
            {
              "type": "STRING",
              "value": "ebx"
            },
            {
              "type": "STRING",
              "value": "esi"
            },
            {
              "type": "STRING",
              "value": "edi"
            },
            {
              "type": "STRING",
              "value": "esp"
            },
            {
              "type": "STRING",
              "value": "ebp"
            }
          ]
        }
      ]
    },
    "index": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "register"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "string": {
      "type": "PATTERN",
      "value": ".*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t]+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
